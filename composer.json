{
    "name": "networkteam/import",
    "description": "Generic import framework",
    "keywords": ["import", "dataprovider", "excel", "csv"],
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Networkteam\\Import\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Networkteam\\Import\\Tests\\": "tests/" }
    },
    "require":{
        "php": "^7.2",
        "psr/log": "~1.0",
        "symfony/expression-language": ">=2.4|~3.0|~4.0|~5.0"
    },
    "suggest": {
        "phpoffice/phpspreadsheet": "Used by the ExcelDataProvider to import Excel files"
    },
    "require-dev": {
        "ext-pdo": "*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.5.2",
        "doctrine/orm": ">=2.4.0",
        "phpoffice/phpspreadsheet": "^1.2.1"
    }
}
